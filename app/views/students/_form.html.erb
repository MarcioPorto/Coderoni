<%= simple_form_for(@student) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :gender %>
    <%= f.input :ethnicity %>
    <%= f.input :military %>
    <%= f.input :academic_interests %>
    <%= f.input :gpa %>
    <%= f.input :sat_cr_score %>
    <%= f.input :sat_w_score %>
    <%= f.input :sat_m_score %>
    <%= f.input :act_e_score %>
    <%= f.input :act_r_score %>
    <%= f.input :act_m_score %>
    <%= f.input :act_s_score %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
